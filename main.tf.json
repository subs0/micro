{
  "data": {
    "aws_iam_policy_document": {
      "ms1_lambda_creds": {
        "statement": {
          "effect": "Allow",
          "actions": [
            "sts:AssumeRole"
          ],
          "principals": {
            "identifiers": [
              "lambda.amazonaws.com",
              "apigateway.amazonaws.com"
            ],
            "type": "Service"
          }
        }
      },
      "ms1_lambda_access_creds": {
        "statement": [
          {
            "effect": "Allow",
            "actions": [
              "s3:AbortMultipartUpload",
              "s3:ListMultipartUploadParts",
              "s3:ListBucketMultipartUploads",
              "s3:PutObject",
              "s3:GetObject",
              "s3:DeleteObject"
            ],
            "resources": [
              "arn:aws:s3:::${resource.aws_s3_bucket.ms1_s3.bucket}",
              "arn:aws:s3:::${resource.aws_s3_bucket.ms1_s3.bucket}/*"
            ]
          },
          {
            "effect": "Allow",
            "actions": [
              "sns:Publish",
              "sns:Subscribe"
            ],
            "resources": [
              "${resource.aws_sns_topic.ms1_topic.arn}"
            ]
          },
          {
            "effect": "Allow",
            "actions": [
              "logs:CreateLogGroup",
              "logs:CreateLogStream",
              "logs:PutLogEvents"
            ],
            "resources": [
              "${resource.aws_cloudwatch_log_group.ms1_cloudwatch.arn}:*",
              "${resource.aws_cloudwatch_log_group.ms1_cloudwatch.arn}:*:*"
            ]
          }
        ]
      }
    },
    "aws_route53_zone": {
      "subdomains_zone": {
        "name": "chopshop-test.net"
      }
    }
  },
  "resource": {
    "aws_cloudwatch_log_group": {
      "ms1_cloudwatch": {
        "name": "/aws/lambda/throwaway-test-123-log-group",
        "retention_in_days": 7
      }
    },
    "aws_iam_policy": {
      "ms1_lambda_policy": {
        "name": "throwaway-test-123-policy-policy",
        "policy": "${data.aws_iam_policy_document.ms1_lambda_access_creds.json}"
      }
    },
    "aws_iam_role": {
      "ms1_lambda_role": {
        "name": "throwaway-test-123-role",
        "assume_role_policy": "${data.aws_iam_policy_document.ms1_lambda_creds.json}"
      }
    },
    "aws_iam_role_policy_attachment": {
      "ms1_lambda_policy_attachment": {
        "role": "${resource.aws_iam_role.ms1_lambda_role.name}",
        "policy_arn": "${resource.aws_iam_policy.ms1_lambda_policy.arn}"
      }
    },
    "aws_s3_bucket": {
      "ms1_s3": {
        "bucket": "throwaway-test-123-bucket"
      }
    },
    "aws_lambda_function": {
      "ms1_lambda": {
        "function_name": "lambda-throwaway-test-123",
        "role": "${resource.aws_iam_role.ms1_lambda_role.arn}",
        "runtime": "python3.8",
        "handler": "handler.handler",
        "filename": "${path.root}/lambdas/template/zipped/handler.py.zip",
        "environment": {
          "variables": {
            "S3_BUCKET_NAME": "${resource.aws_s3_bucket.ms1_s3.bucket}",
            "SNS_TOPIC_ARN": "${resource.aws_sns_topic.ms1_topic.arn}"
          }
        }
      }
    },
    "aws_sns_topic": {
      "ms1_topic": {
        "name": "throwaway-test-123-topic"
      }
    },
    "aws_lambda_permission": {
      "ms1_sns_invoke_cred": {
        "statement_id": "AllowExecutionFromSNS",
        "action": "lambda:InvokeFunction",
        "function_name": "${resource.aws_lambda_function.ms1_lambda.function_name}",
        "principal": "sns.amazonaws.com",
        "source_arn": "${resource.aws_sns_topic.ms1_topic.arn}"
      }
    },
    "aws_sns_topic_subscription": {
      "ms1_subscription": {
        "topic_arn": "${resource.aws_sns_topic.ms1_topic.arn}",
        "protocol": "lambda",
        "endpoint": "${resource.aws_lambda_function.ms1_lambda.arn}",
        "filter_policy": "{\"type\":[\"type1\",\"type2\"]}",
        "filter_policy_scope": "MessageAttributes"
      }
    },
    "aws_acm_certificate": {
      "subdomains_cert_0": {
        "domain_name": "chopshop-test.net",
        "validation_method": "DNS",
        "subject_alternative_names": [
          "test1.chopshop-test.net"
        ],
        "tags": {
          "BroughtToYouBy": "@-0/micro"
        },
        "lifecycle": {
          "create_before_destroy": true
        }
      }
    },
    "aws_acm_certificate_validation": {
      "subdomains_validation_0": {
        "certificate_arn": "${resource.aws_acm_certificate.subdomains_cert_0.arn}"
      }
    },
    "aws_apigatewayv2_domain_name": {
      "subdomains_domain_0": {
        "domain_name": "test1.chopshop-test.net",
        "domain_name_configuration": [
          {
            "certificate_arn": "${resource.aws_acm_certificate.subdomains_cert_0.arn}",
            "endpoint_type": "REGIONAL",
            "security_policy": "TLS_1_2"
          }
        ],
        "tags": {
          "Name": "test1.chopshop-test.net",
          "BroughtToYouBy": "@-0/micro"
        }
      }
    },
    "aws_route53_record": {
      "subdomains_record_0": {
        "name": "test1",
        "zone_id": "${data.aws_route53_zone.subdomains_zone.zone_id}",
        "type": "A",
        "alias": {
          "name": "${resource.aws_apigatewayv2_domain_name.subdomains_domain_0.domain_name_configuration[0].target_domain_name}",
          "zone_id": "${resource.aws_apigatewayv2_domain_name.subdomains_domain_0.domain_name_configuration[0].hosted_zone_id}",
          "evaluate_target_health": false
        }
      }
    },
    "aws_apigatewayv2_api": {
      "subdomains_gateway_0": {
        "name": "test1",
        "description": "api for test1",
        "disable_execute_api_endpoint": false,
        "protocol_type": "HTTP",
        "cors_configuration": {
          "allow_headers": [
            "content-type",
            "x-amz-date",
            "authorization",
            "x-api-key",
            "x-amz-security-token",
            "x-amz-user-agent"
          ],
          "allow_methods": [
            "*"
          ],
          "allow_origins": [
            "*"
          ],
          "max_age": 300
        }
      }
    },
    "aws_apigatewayv2_stage": {
      "subdomains_stage_0": {
        "api_id": "${resource.aws_apigatewayv2_api.subdomains_gateway_0.id}",
        "name": "$default",
        "auto_deploy": true,
        "description": "stage $default API"
      }
    },
    "aws_apigatewayv2_integration": {
      "subdomains_integration_0_0": {
        "api_id": "${resource.aws_apigatewayv2_api.subdomains_gateway_0.id}",
        "integration_uri": "${resource.aws_lambda_function.ms1_lambda.invoke_arn}",
        "integration_type": "AWS_PROXY",
        "integration_method": "POST",
        "connection_type": "INTERNET",
        "payload_format_version": "2.0",
        "timeout_milliseconds": 29000
      }
    },
    "aws_apigatewayv2_route": {
      "subdomains_route_0_0": {
        "api_id": "${resource.aws_apigatewayv2_api.subdomains_gateway_0.id}",
        "route_key": "ANY /",
        "target": "integrations/${resource.aws_apigatewayv2_integration.subdomains_integration_0_0.id}"
      }
    }
  },
  "terraform": {
    "required_providers": {
      "aws": {
        "source": "hashicorp/aws",
        "version": "5.20.0"
      }
    }
  },
  "provider": [
    {
      "aws": {
        "region": "us-east-2",
        "profile": "chopshop"
      }
    }
  ]
}