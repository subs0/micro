{
   "name": "zipped",
   "runtime": "python3.11",
   "handler": "index.handler",
   "architectures": ["x86_64"],
   "bucket": true,
   "memory": 1024,
   "timeout": 120,
   "sns": {
      "upstream": {
         "topic": "topic_key",
         "filter_policy": {
            "type": ["lambda"]
         }
      },
      "downstream": {
         "topic": "topic_key",
         "message_attrs": {
            "type": {
               "DataType": "String",
               "StringValue": "lambda"
            }
         }
      }
   },
   "api": {
      "subdomain": "up"
   },
   "tags": {
      "welcome": "home"
   }
}
